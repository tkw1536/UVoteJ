{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [".", "README.md"],
        "exclude": ["node_modules/"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "doc/",
        "recurse": true
    }
}
