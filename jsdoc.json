{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["backend", "README.md"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "doc",
        "recurse": true
    }
}
